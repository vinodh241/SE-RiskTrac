        </svg>
    </div>
    <div>
        <h4 class="ms-10">
            <span (click)="navigatePage()" style="cursor:pointer;">
                <span style="color: #047bc5;">
                    <strong>Risk Appetite</strong>
                </span>&nbsp;
                <span style="font-weight: 400;"> - &nbsp;{{this.viewall}} Wise</span>
            </span>
        </h4>
    </div>
    <mat-card (click)="navigatePage()" style="cursor:pointer;">
        <div class="toggle-button toggleInRAOnly">
            <span (click)="$event.stopPropagation()" style="cursor:context-menu;">Group&nbsp;&nbsp;</span>
            <mat-slide-toggle (click)="$event.stopPropagation()" (change)="toggleValues($event)"
                name="status"></mat-slide-toggle>
            <span (click)="$event.stopPropagation()" style="cursor:context-menu;">&nbsp;&nbsp;Unit</span>
        </div>
        <div class="matcard1" style="display:flex;justify-content:space-between;cursor:pointer;">
            <div style="padding-left:2vh;">
                <mat-card class="matcard-card">
                    <mat-card-content>
                        <div class="">
                            <div class="" style="font-size:19px;margin-bottom:0.7vh;margin-top: -1.2vh;">
                                <strong>Average # of Risk Metrics</strong>
                            </div>
                            <div style="padding-bottom:0.5vh;"><strong
                                    style="font-size: 16px;color: #B10000;">CRITICAL</strong> Risk Metrics<span
                                    style="padding-left:4.5vh;font-size: 17px;;color: #B10000;"><strong>{{criticalData}}</strong></span>
                            </div>
                            <div style="padding-bottom:0.5vh;"><strong
                                    style="font-size: 16px;color: #FF6E05;">MODERATE</strong> Risk Metrics<span
                                    style="padding-left:3vh;font-size: 17px;color: #FF6E05;"><strong>{{moderateData}}</strong></span>
                            </div>
                            <div style="padding-bottom:0.3vh;"><strong
                                    style="font-size: 16px;color: #0B9A45;">LOW</strong> Risk Metrics<span
                                    style="padding-left:8.8vh;font-size: 17px;color: #0B9A45;"><strong>{{lowData}}</strong></span>
                            </div>
                        </div>
                    </mat-card-content>
                </mat-card>
            </div>
            <div
                style="height: 110px;overflow-y: auto;overflow-x: hidden;width: 300px; cursor:pointer;padding-right: 10px;margin-top: 5px;">
                <!-- <div id="overallRaGraph" style='width: 290px'></div> -->
                <div class="progrescontainer" *ngFor="let data of raGraphData">
                    <div class="mainprogress">
                        <div class="bar">
                            <div class="contanrside">{{data.name}}</div>

                            <div class="mainbar">
                                <div class="progress" role="progressbar" aria-label="Segment one"
                                    [matTooltip]="getToolTip(data.CriticalRiskLevel, 'Critical')"
                                    style="background-color:#B10000 ;"
                                    [style.width.%]="(data.CriticalRiskLevel / totalRiskLevel(data)) * 100">
                                </div>
                                <div class="gap" *ngIf="(data.CriticalRiskLevel > 0) && (data.ModerateRiskLevel > 0)"></div>
                                <div class="progress" role="progressbar" aria-label="Segment two"
                                    [matTooltip]="getToolTip(data.ModerateRiskLevel, 'Moderate')"
                                    style="background-color:#FF6E05;"
                                    [style.width.%]="(data.ModerateRiskLevel / totalRiskLevel(data)) * 100">
                                </div>
                                <div class="gap" *ngIf="(data.CriticalRiskLevel > 0 || data.ModerateRiskLevel > 0) && data.LowRiskLevel > 0"></div>
                                <div class="progress" role="progressbar" aria-label="Segment three"
                                    [matTooltip]="getToolTip(data.LowRiskLevel, 'Low')"
                                    style="background-color:#0B9A45;"
                                    [style.width.%]="(data.LowRiskLevel / totalRiskLevel(data)) * 100">
                                </div>
                            </div>
                        </div>


