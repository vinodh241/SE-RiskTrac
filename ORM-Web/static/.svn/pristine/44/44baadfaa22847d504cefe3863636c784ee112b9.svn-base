<span><b>Overall Inherent Risk</b></span>
<mat-card class="mat-elevation-z4" >
    <mat-card-header>
    </mat-card-header>
    <mat-card-content>
        <div class="content">
            <span><b>Computation Formula</b></span>
            <div class="tag-box" >
                <ul  fxLayout="row wrap">
                    <li *ngFor="let item of tags" >
                        {{item?.Description}}  <mat-icon *ngIf="iseditdg" (click)="RemoveData(item)">close</mat-icon>
                    </li>
                </ul>
            </div>
        </div>
    </mat-card-content>
    <mat-card-actions>
        <div fxLayout="row" fxLayoutAlign="end end">
            <div class="content" fxLayout="row wrap"  fxLayoutAlign="start center">
                
                <div class="tag-box-items" *ngIf="iseditdg" >
                    <span *ngIf="iseditdg"><b>Scores</b></span>
                    <ul  fxLayout="row wrap">
                        <li *ngFor="let a of sourcedatascore; let index = index" (click)="addtagitem(a)">
                           {{ a.Description }}
                        </li>
                    </ul>
                </div>
            
            <div class="tag-box-items" *ngIf="iseditdg" >
                <span *ngIf="iseditdg"><b>Operstors</b></span>
                <ul  fxLayout="row wrap">
                    <li *ngFor="let a of sourcedataoperator; let index = index" (click)="addtagitem(a)">
                       {{ a.Description }}
                    </li>
                </ul>
            </div>
            <button fxFlexOffset="10px" mat-mini-fab color="warn" aria-label="Save"  *ngIf="iseditdg" >
                <mat-icon>check</mat-icon>
            </button>
            <button mat-mini-fab fxFlexOffset="10px" class="buttongray" aria-label="Cancel" (click)="Cancel()" *ngIf="iseditdg">
                <mat-icon>close</mat-icon>
            </button>
        </div>
            <button mat-icon-button *ngIf="!iseditdg">
                <mat-icon (click)="editinitiate()" aria-hidden="false" aria-label="Edit">edit
                </mat-icon>
            </button>
           
        </div>
    </mat-card-actions>
  </mat-card>

