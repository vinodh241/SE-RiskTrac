    justify-content: center;
    align-items: center;
    font-weight: 700;
}

.historical-text {
    padding-left: .5vw;
    color: rgb(88, 160, 241);
}


.tooltip {
    position: relative;
    display: inline-block;
}

.tooltip .tooltiptext {
    visibility: hidden;
    width: 20vw;
    background-color: black;
    color: #fff;
    border-radius: 6px;
    padding: 5px 0;
    position: absolute;
    right: auto;
    z-index: 1;
    right: 0%;
}

.tooltip .tooltiptext::after {
    content: "";
    position: absolute;
    bottom: 100%;
    right: 5%;
    border-width: 1.2vh;
    border-style: solid;
    width: .3vw;
    height: .3vh;
    transform: rotate(-90deg);
    border-color: transparent transparent transparent black;
}

.tooltip:hover .tooltiptext {
    visibility: visible;
}

.mat-toolbar {
    height: 6.4vh;
    // margin-left: 30px;
}

th.mat-header-cell,
td.mat-cell {
