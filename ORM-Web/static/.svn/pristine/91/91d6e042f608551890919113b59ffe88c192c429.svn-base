    background: #e5e5e5;
}
.top-padding{
    padding-top: 13vh;
}


.mat-form-field-appearance-fill .mat-form-field-flex {
    width: 200px !important;
}
.dashFilter{
    padding-top: 1vh;
    width: 100% !important;
    height: 9px !important;
}
.dashFilter mat-card-content{
    display: flex;
    border-bottom: 1px solid #a7a7a7;
}
.dashFilter, .mat-form-field-infix {
    top: 0px !important;
    width: 100% !important;
    border-radius: 4px 4px 0 0 !important;
}

.dashFilter mat-card-content .w50{
    width: calc(50% - 1vh);
    margin-right: 10px;
}
.dashFilter mat-form-field{
    width: 100%;
}

.custom-select .mat-select-arrow-wrapper {
    display: none;
  }

  .custom-select .mat-select-value {
    display: flex;
    align-items: center;
  }

  .module-text {
    pointer-events: none;
  }

  .radio-group {
    display: flex;
    flex-direction: column;
    row-gap: 0.5em;
    padding: 0.5em;
    background: white;
    margin-bottom: 1em;
  }

  .mat-form-field-appearance-fill .mat-form-field-underline::before {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    /* height: 1px; */
    width: 100%;
}
.mat-date-range-input {
    display: block;
    width: 100%;
    margin-bottom: 5vh !important;
}
mat-form-field {
    width: 100%; /* Ensure the form field takes full width */
}

mat-select {
    display: flex;
    align-items: center; /* Align items properly */
}

.mat-form-field-infix {
    min-height: auto !important; /* Reduce unnecessary space */
}

.mat-select-placeholder {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}
