.side-card-header {
    background: #12121212;
    padding: 10px 1px;
    color: white;
}

.side-card-title {
    font-size: 17px !important;
}
.header-style{
    margin: 5px;
    font-size: 15px;
    font-weight: 500;
}

.side-card-content {
    padding: 0px 10px;
    color: black;
}

.icon-excel {
    min-width: 0px;
    border-radius: 20px;
}

.circle {
    display: inline;
    background: green;
    padding: 20px;
    border-radius: 50%;
}

.dot {
    height: 50px;
    width: 50px;
    background-color: red;
    border-radius: 50%;
}

