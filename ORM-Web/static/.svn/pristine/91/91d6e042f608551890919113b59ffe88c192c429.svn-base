.container{
    background: #e3e3e3;
}

.upload-button {
    font-size: 1.3vh;
    background-color: #2470A3;
    color: #fff;
    border-radius: 12px;
    cursor: pointer;

}


.reporting-frequency-font {
    font-weight: bold;
}

.historical {
    background-color: transparent;
    border: none;
    cursor: pointer;
    color: #fff;
}

.dropdown-label {
    margin-right: 0.5rem;
}

.measured-text-space {
    background-color: transparent;
    border: none;
    cursor: pointer;
    color: #fff;
    width: 100%;
    display: flex;
    justify-content: space-between;
    background-color: transparent;
}

.measured {
    background-color: transparent;
    border: none;
    cursor: pointer;
    color: #fff;
    margin-left: 10px;
}

.editable {
    width: 4.5vw;
}

.measured-text {
    background-color: transparent;
    border: none;
    cursor: pointer;
    color: #fff;

}

.text-align-center {
    text-align: center;
}

.value-container {
    height: 40px;
    width: 40px;
    border-radius: 24px;
