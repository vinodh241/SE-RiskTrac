tr.Tableheader th {
  border: 1px solid #CCC;
  text-align: center;
}

.ScheduleGroup {
  background: rgb(36, 112, 163);
  background: -moz-linear-gradient(180deg, rgba(36, 112, 163, 1) 45%, rgba(100, 181, 191, 0.9790575916230366) 100%);
  background: -webkit-linear-gradient(180deg, rgba(36, 112, 163, 1) 45%, rgba(100, 181, 191, 0.9790575916230366) 100%);
  background: linear-gradient(180deg, rgba(36, 112, 163, 1) 45%, rgba(100, 181, 191, 0.9790575916230366) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#2470a3", endColorstr="#64b5bf", GradientType=1);
}

.mat-toolbar {
    height: 6.4vh;
}

.mat-elevation-z8 {
  margin: 0vh 4vh 0 4vh;
}

.mat-action-row {
  margin: 0vh 4vh 0 4vh;
}

.mat-header-row {
  min-height: 10vh;
}

.mat-column-RowNumber {
  max-width: 2vw;
}

.mat-column-Risk,
.mat-column-ControlDescription {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 150px;
}

.mat-column-OverallInherentRiskRating {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 120px;
}

.mat-column-Group,
.mat-column-IdentifiedAction,
.mat-column-Comments,
.mat-column-Comments1,
.mat-column-Timeline,
.mat-column-RCSACode {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  // height: 50px;
  max-width: 100px;
}


.ScheduleGrouphead>span {
  background-color: #2980b900;
  color: white;
  margin-top: 2vh;
  height: 6vh;
  font: 300 3vh / 1 Roboto Condensed;
}

