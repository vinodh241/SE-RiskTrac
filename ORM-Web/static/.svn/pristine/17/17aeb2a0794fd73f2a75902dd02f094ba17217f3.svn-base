<div
    style="height: 100%;display: flex; height: '80vh';  width: '70vw'; flex-direction:column; justify-content:space-between">
    <div>
        <mat-toolbar
            style="display: flex; justify-content:space-between ;align-items: center; background-image: linear-gradient(to left ,#009873, #0B65A0);">
            <div style="padding-left: 10px;">{{title}}</div>
            <button mat-icon-button title="Close" cdkFocusInitial mat-dialog-close>
                <mat-icon aria-hidden="false" aria-label="Close" fontIcon="close"></mat-icon>
            </button>
        </mat-toolbar>
        <div class="mat-table-padding data-container">
            <table mat-table [dataSource]="dataSource" matSort style="width: 100%;" class="mat-elevation-z8">
                <ng-container matColumnDef="Position">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;;width: 0.5vw;border: 2px solid white;"> #</th>
                    <td mat-cell *matCellDef="let element  let i = index"> {{i+1}}</td>
                </ng-container>

                <ng-container matColumnDef="QuarterWisePosition">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 4vw;border: 2px solid white;padding-left: 10px;">Quarter
                        Wise Position</th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;"> {{element.Quater}} </td>
                </ng-container>
                <ng-container matColumnDef="IncidentCode">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 4vw;border: 2px solid white;padding-left: 10px;">Incident
                        Code</th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;">
                        <label class="link" (click)="openIncident(element.IncidentID)"> {{element.IncidentCode}}</label></td>
                </ng-container>

                <ng-container matColumnDef="IncidentTitle">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 5vw;border: 2px solid white;padding-left: 10px;">Incident
                        Title</th>
                    <td mat-cell *matCellDef="let element"  [matTooltip]="element.IncidentTitle" style="padding-left: 10px;"> {{element.IncidentTitle}} </td>
                </ng-container>

                <ng-container matColumnDef="Status">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 3vw;border: 2px solid white;padding-left: 10px;">Status
                    </th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;"> {{element.StatusName}} </td>
                </ng-container>

                <ng-container matColumnDef="IncidentDate">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 3vw;border: 2px solid white;padding-left: 10px;">Incident
                        Date</th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;">
                        {{utils.formatDate(element.IncidentDate)}} </td>
                </ng-container>

                <ng-container matColumnDef="ReportingDate">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 3vw;border: 2px solid white;padding-left: 10px;">
                        Reporting Date</th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;">
                        {{utils.formatDate(element.CreatedDate)}} </td>
                </ng-container>

                <ng-container matColumnDef="Amount">
                    <th mat-header-cell *matHeaderCellDef
                        style="background-color:#e4e4e4;width: 3vw;border: 2px solid white;padding-left: 10px;">
                        Amount<span style="font-weight:500 ;">&nbsp;in {{currency}}(If Any)</span></th>
                    <td mat-cell *matCellDef="let element" style="padding-left: 10px;"> {{element.LossAmount}} </td>
                </ng-container>

                <tr mat-header-row *matHeaderRowDef="displayedColumns" style="background-color:  rgb(207, 205, 205);">
                </tr>
                <tr mat-row *matRowDef="let row; let i = index columns: displayedColumns;"></tr>

            </table>
        </div>
    </div>

    <!-- <div class="bottom-dig">
        <button mat-stroked-button mat-dialog-close>Cancel</button>
    </div> -->

    <div class="modal-footer modalFooter">
        <!-- <button mat-stroked-button mat-dialog-close>Cancel</button> -->
        <button type="button" class="btn btn-secondary modalCancelBtn" id="cancel" style="margin-right: 1vh" mat-dialog-close>Cancel</button>
    </div>

</div>
