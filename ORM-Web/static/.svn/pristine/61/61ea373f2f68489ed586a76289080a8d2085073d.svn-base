.bsswitch input:focus + .bsslider {box-shadow: 0 0 1px #2196F342 !important;}
.bsswitch input:checked + .bsslider:before { -webkit-transform: translateX(20px); -ms-transform: translateX(20px); transform: translateX(20px);}
