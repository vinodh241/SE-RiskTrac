.bsswitch .bsslider:before {position: absolute; content: ""; height: 20px; width: 20px; left: 4px; bottom: -4px; background-color: rgb(41, 99, 118); -webkit-transition: .4s; transition: .4s;    border: 1px solid #bde1ff61;}
