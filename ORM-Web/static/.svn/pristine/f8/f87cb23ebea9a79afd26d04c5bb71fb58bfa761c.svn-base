<mat-toolbar class="fontSize">
    <span>Control Environment Rating</span>
    <span class="spacer"></span>
</mat-toolbar>
<div class="container ">
    <div class="master-table-container" fxLayout="row wrap" fxLayoutGap="2% grid" fxFlex="98%" fxFlexOffset.lg="1%" fxLayoutAlign="start stretch" >
      <div fxFlex.lg="32%" fxFlex.md="48%" fxFlex.sm="100%" fxFlex.xs="100%" fxFlexOffset.lg="1%" fxFlexOffset.md="1%">
        <app-control-in-pace></app-control-in-pace>
      </div>

      <div fxFlex="33%" fxFlex.md="50%" fxFlex.sm="100%" fxFlex.xs="100%">
        <app-control-nature-score></app-control-nature-score>
      </div>

      <div fxFlex="33%" fxFlex.md="48%" fxFlex.sm="100%" fxFlex.xs="100%" fxFlexOffset.md="1%">
        <app-control-automation-score></app-control-automation-score>

      </div>
      <div fxFlex.lg="32%" fxFlex.md="50%" fxFlex.sm="100%" fxFlex.xs="100%" fxFlexOffset.lg="1%">
        <app-control-frequency-score></app-control-frequency-score>
      </div>
      <div fxFlex.lg="33%" fxFlex.md="50%" fxFlex.sm="100%" fxFlex.xs="100%">
        <app-control-total-score></app-control-total-score>
      </div>

    </div>
    </div>

