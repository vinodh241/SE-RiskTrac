.rd-tbl {
    width: 100%;
    box-shadow: 0px 4px 5px rgba(0, 0, 0, 0.45);
    border-top-left-radius: 1vmin;
    border-top-right-radius: 1vmin;
    border-bottom-left-radius: 1vmin;
    border-bottom-right-radius: 1vmin;
    // overflow: hidden;
    border-collapse: collapse;
}
.rd-tbl-ordhead {
   width: 100%;
}
.rd-tbl td,
.rd-tbl th {
    padding: 0.4rem;
    vertical-align: middle;
    //text-align: left;
}
.rd-tbl-ordbody {
    background-image: linear-gradient(to right, #dad9d9, #fff);
}
.rd-tbl-ordbody td:nth-child(2),
.rd-tbl-ordbody td:nth-child(3) {
    font-size: 200%;
}
.rd-tbl-ordbody td {
    border-bottom: 1px solid #c6c6c6;
}

.rd-tbl tr:hover td {
    background-color: #fffdeb;
}
.rd-tbl tr:nth-child(odd) {
    background-color: #f3f3f3;
}



.mat-header-row {
    background-image: linear-gradient(to right, #a8d7f5, #ffbfbf);
  }


.mat-tool-bar {
    width: 100%;
    display: flex;
    justify-content: end;
    align-items: center;
}

::ng-deep.mat-dialog-container {
    padding: 0px;

}

.mat-table-data {
    margin-left: 20px;
    margin-right: 20px;
    margin-bottom: 20px;
    margin-top: 20px;
}

::ng-deep.mat-elevation-z8 {
    box-shadow: none;
}

::ng-deep.mat-table {
    overflow-x: scroll;
}


  
  .mat-table {
    overflow: auto;
  }
  


table {
    width: 100%;
}

.data-container {
    height: 400px;
    max-width: 100%;
    overflow: auto;
}


.bottom-dig{
    background-color: #e7e7e7;
    padding: 20px;
    display: flex;
    align-items: center;
    justify-content: right;
    height: 45px;
}
.bottom-dig button{
    background-color: #c9c9c9;
    color: black;
    border-radius: 0px;
    border: 0px;
    box-shadow: 0px 1px #777777;
    overflow: hidden;
}
.rd-tbl-data {
    color: #727272;
}
.rd-tbl-data:hover{
    text-decoration: none;
    color: #000;
}