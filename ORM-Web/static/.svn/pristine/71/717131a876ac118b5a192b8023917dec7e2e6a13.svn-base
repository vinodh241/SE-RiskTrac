<div style="height: 100%;display: flex; height: '80vh';  width: '80vw'; flex-direction:column; justify-content:space-between">
    <div >
        <mat-toolbar style="display: flex; justify-content:space-between ;align-items: center; background-image: linear-gradient(to left ,#009873, #0B65A0);" >
            <div>{{popUpName}} - {{RANAME}} - {{ statusRisk ? statusRisk : ''}}</div>
            <button mat-icon-button title="Close" cdkFocusInitial mat-dialog-close>
                <mat-icon aria-hidden="false" aria-label="Close" fontIcon="close"></mat-icon>
            </button>
        </mat-toolbar>
        <div class="mat-table-padding data-container">

                <table mat-table [dataSource]="dataSource"  matSort style="width: 100%;" class="mat-elevation-z8">
                    <ng-container matColumnDef="No">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px; width:1%;"> #</th>
                        <td mat-cell *matCellDef="let element let i = index"  style="padding-left: 10px;"> {{i+1}} </td>
                    </ng-container>
                    <ng-container matColumnDef="FrameworkName">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;width:12%;">Framework Name</th>
                        <td mat-cell *matCellDef="let element"  style="padding-left: 10px;"> {{element.FrameworkName}} </td>
                    </ng-container>
                    <ng-container matColumnDef="Quater">
                        <th mat-header-cell *matHeaderCellDef style=" border-left: 1px solid #fff; padding-left: 10px;">Quarter</th>
                        <td mat-cell *matCellDef="let element" style="padding-left: 10px;"> {{element.Quater}} </td>
                    </ng-container>
                    <ng-container matColumnDef="StartDate">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;">Start Date</th>
                        <td mat-cell *matCellDef="let element"  style="padding-left: 10px;"> {{formatDate(element.StartDate)}} </td>
                    </ng-container>
                    <ng-container matColumnDef="EndDate">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;">End Date</th>
                        <td mat-cell *matCellDef="let element"  style="padding-left: 10px;"> {{formatDate(element.EndDate)}} </td>
                    </ng-container>
                    <ng-container matColumnDef="GroupName">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px; width:15%;"> Group </th>
                        <td mat-cell *matCellDef="let element"  style="padding-left: 10px;"> {{element.GroupName}} </td>
                    </ng-container>
                    <ng-container matColumnDef="UnitName">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px; width:10%;">Unit</th>
                        <td mat-cell *matCellDef="let element" style="padding-left: 10px;">{{element.UnitName}}</td>
                    </ng-container>
                    <ng-container matColumnDef="Risks">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;width:25%;">Risk Metric</th>
                        <td mat-cell *matCellDef="let element" style="padding-left: 10px;">{{element.Risks}}</td>
                    </ng-container>
                    <ng-container matColumnDef="MetricScore">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;">Score</th>
                        <td mat-cell *matCellDef="let element" style="padding-left: 10px;">{{element.MetricScore}}</td>
                    </ng-container>
                    <ng-container matColumnDef="CollectionStatusName">
                        <th mat-header-cell *matHeaderCellDef style="border-left: 1px solid #fff; padding-left: 10px;">Status</th>
                        <td mat-cell *matCellDef="let element"  style="padding-left: 10px;"> {{element.CollectionStatusName}} </td>
                    </ng-container>
                    <tr mat-header-row *matHeaderRowDef="displayedColumns" style="background-color:  rgb(207, 205, 205);"></tr>
                    <tr mat-row *matRowDef="let row; columns: displayedColumns;"  ></tr>
                </table>


            <!-- <mat-paginator #paginator [pageSize]="5" showFirstLastButtons [pageSizeOptions]="[5,10, 25, 100]"></mat-paginator> -->
        </div>
    </div>
    <div class="modal-footer modalFooter">
        <!-- <button mat-stroked-button mat-dialog-close>Cancel</button> -->
        <button type="button" class="btn btn-secondary modalCancelBtn" id="cancel" style="margin-right: 1vh" mat-dialog-close>Cancel</button>
    </div>

</div>



