<mat-toolbar style="width: auto; margin:-24px -24px 0px -24px; height: 8.5vh; font-size: 3.3vh;">
  <h2 mat-dialog-title>{{screenData[0].Title}} - In Progress</h2>
  <span class="spacer"></span>
  <button mat-icon-button title="Close" cdkFocusInitial mat-dialog-close>
    <mat-icon aria-hidden="false" aria-label="Close" fontIcon="close"></mat-icon>
  </button>
</mat-toolbar>
<mat-dialog-content>
  <div class="container" fxLayout="row wrap">
    <div fxLayout="column" fxFlex="70">
      <h2><b>Description</b></h2>
      <p>{{screenData[0].AssessmentDescription}}</p>
    </div>
    <div class="Proposeddateinfo" fxLayout="column" fxFlex="30">
      <b>Proposed Start Date</b>
      <span>{{screenData[0].ProposedStartDate | date:'dd MMM yyyy'}}</span>
      <b>Proposed End Date</b>
      <span>{{screenData[0].ProposedCompletionDate | date:'dd MMM yyyy'}}</span>
    </div>
  </div>
  <div class="container">
    <div fxLayout="row wrap" fxLayoutGap="10px grid" fxFlex="98%" fxLayoutAlign="start stretch">
      <mat-card class="mat-elevation-z4" fxLayout="column" fxFlex="100">
        <mat-card-content fxFlex="100" fxLayout="row">
          <table mat-table [dataSource]="dataSource">
            <ng-container *ngFor="let column of displayedKeyValueHeaders" [matColumnDef]="column.HeaderKey">
              <th mat-header-cell rowspan="2" *matHeaderCellDef>{{column.HeaderValue}}</th>
              <td mat-cell *matCellDef="let object;">{{object[column.HeaderKey]}}</td>
            </ng-container>
            <ng-container *ngFor="let column of displayedKeyValueDetailed" [matColumnDef]="column.HeaderKey">
              <th mat-header-cell *matHeaderCellDef>{{column.HeaderValue}}</th>
              <td mat-cell *matCellDef="let object;">{{object[column.HeaderKey]}}</td>
            </ng-container>
            <ng-container matColumnDef="DetailedStatus">
              <th mat-header-cell *matHeaderCellDef style="text-align: center;" [attr.colspan]="detailedStatusColSpan">
                {{screenData[0].FirstHeader}}
              </th>
              <td mat-cell *matCellDef="let row;"></td>
            </ng-container>

            <tr mat-row *matHeaderRowDef="displayedHeaders"></tr>
            <tr mat-row *matHeaderRowDef="displayedDetailedHeaders"></tr>
            <tr mat-row *matRowDef="let element; columns:displayedColumns"></tr>
          </table>

        </mat-card-content>
        <div fxFlex="row" fxLayoutAlign="center center">
          <span class="Errorinfo">{{datanotFound}}</span>
        </div>
      </mat-card>
    </div>

  </div>
</mat-dialog-content>