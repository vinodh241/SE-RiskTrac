
.mainClassRow{
    display: flex;
    flex-direction: column;
    height: calc(100vh - 18.8vh);
}
.mat-action-row { 
    border: 0px;
    justify-content: flex-start;
    position: relative;
    padding: 0px;    
    align-items: inherit;
    height: calc(33.33% - 4vh - 10px);
    margin: 4vh 0 0 0;
}  

.matInnerClassFirst {
    margin: 0 1.33vh 0 0;
    width: calc(33.33% - 2.66vh);
}
.matInnerClassSecond {
    margin: 0 2.66vh 0 2.66vh;
    width: calc(33.33% - 2.66vh);
}
.matInnerClassThird {
    margin: 0 0 0 1.33vh;
    width: calc(33.33% - 2.66vh);
}
.mat-card {
    height: calc(100% - 32px);  
}
.mat-card-content{ 
    height: 100%;
}
.thirdSecLast{
    display: flex; 
    justify-content: space-between;
    height: calc(100% - 45px);                    
    align-items: center;
}
