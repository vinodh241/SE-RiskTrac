.info1 div {
    font-weight: normal;
    color: #000;
    font-size: 13px;
    // margin-bottom: 0;
}

.info1 h3 {
    font-weight: bold;
    color: #fff;
    //font-size: 170%;
    font-size: 1.5em;
    // font-family: "Open Sans Condensed";
    vertical-align: middle;
    margin-bottom: 0;
}

.infoprogresswrapper {
    background-color: rgba(255, 255, 255, 0.2);
    height: 1em;
    border-radius: 50px;
    width: 100%;
    overflow: hidden;
}
.col-4{
    width: calc(33.33% - 2.66vh)
}
.col-8{
    width: calc(66.66% - 1vh);
    margin-left: 4vh;
}
.row{
    margin: 0 0 4vh 0;
}
.row:last-child {
    margin: 0 0 0 0;
}
.infoprogress {
    background-color: #1FAEFF;
    /*height:1em;*/
    border-radius: 50px;
    font-size: 82% !important;
    padding-left: 2%;
    color: #404040;
    line-height: 1.2;
    font-weight: bold !important;
}

::ng-deep .mat-progress-bar-fill::after {
    // background-color: #df893e;
    background-image: linear-gradient(to bottom right, #FFAA2E, #CB4900);
    display: block;
    border-radius:50vh;
}

::ng-deep .mat-progress-bar-buffer {
    background: #E4E8EB;
}

.rd-greybox {
    background-color: #F1F1F1;
    height: 100px;
    width: 100px;
    border-radius: 20px;
    text-align: center;
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
    position: relative;
}

.rd-greybox-inner {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #CB4900;
    font-weight: bold;
    font-size: 300%;
    text-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
}

.rd-greybox .stroke-text {
    font-size: 70px;
    text-align: center;
    text-anchor: middle;
    stroke: #fff;
    stroke-width: 2px;
    fill: #C55D04;
}

.rd-redbox {
    background-image: linear-gradient(#E5C7D7, #F6F6F6);
    height: 100px;
    width: 100px;
    border-radius: 20px;
    text-align: center;
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
    position: relative;
}

.rd-redbox-inner {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.rd-redbox .rd-redbox-inner div {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    vertical-align: middle;
}

.rd-redbox .rd-redbox-inner div:nth-child(1) {
    font-weight: bold;
    color: #C55D04;
    font-size: 200%;
    line-height: 90%;
}

.rd-redbox .rd-redbox-inner div:nth-child(2) {
    color: #2d2d2d;
    font-size: 140%;
    line-height: 90%;
}

