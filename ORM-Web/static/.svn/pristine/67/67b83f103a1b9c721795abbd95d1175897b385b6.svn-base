.mat-toolbar {
    height: 6.4vh;
}

.mat-elevation-z8 {
    margin: 4vh 4vh 0 4vh;
}

.mat-action-row {
    margin: 0vh 4vh 0 4vh;
}

.mat-header-row {
    min-height: 10vh;
}

.mat-column-Index {
    max-width: 2vw;
}

.mat-column-StartDate,
.mat-column-EndDate {
    max-width: 10vh;
}

.mat-column-XlsFile,
.mat-column-PdfFile {
    max-width: 10vh;
}

.mat-column-StatusName,
.mat-column-Action {
    max-width: 10vh;
}

.mat-column-ReviewerName,
.mat-column-UnitName {
    max-width: 40vh;
}

.container{
    margin-top: 30px;
}

.buttongray {
    --mdc-fab-container-color: rgba(0, 0, 0, .12);
    --mdc-fab-icon-color: rgba(0, 0, 0, .38);
    --mat-mdc-fab-color: rgba(0, 0, 0, .38);
}


.mat-row:nth-child(even){
    background-color: #ced2d04d !important;
  }
  .mat-row:not(:nth-child(odd)){
    background-color:#ffffff;
  }
  to

  .mat-row:nth-child(2n+1){
    background-color: #ced2d04d !important;
  }
  .mat-row:not(:nth-child(4n+1)){
    background-color:#ffffff;
  }

  mat-table.mat-table {
    margin-top: 10px;
    max-height: 220px;
    min-height: 220px;
    overflow: auto;
  }
