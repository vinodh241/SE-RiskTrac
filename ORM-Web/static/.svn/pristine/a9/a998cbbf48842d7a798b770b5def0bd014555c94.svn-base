.matbutton{
    position: absolute;
    right: 15px;
    bottom: 2px;
    color: blue;
    font-style: italic;
    text-decoration: underline;
    cursor: pointer;
        font-size: 0.8rem;
    color: rgb(56, 104, 206);
    font-style: italic;
font-family: 'Roboto Condensed'
}

.rd-cmgroup {
    height: 1.2rem;
    border-radius: 100vw;
    position: relative;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
    padding: 0.1px;
    background-color: #fff;
    display: table;
    width: 15rem;
}
.rd-cmgroup-gn {
    height: 1.9rem;
    background-image: linear-gradient(to right, #dad9d9, #ffffff);
    border-radius: 50vw;
    vertical-align: middle;
    display: table-cell;
    font-weight: bold;
    padding-left: 0.8rem;
    padding-bottom: 0.1rem;
}
.rd-cmgroup-fig {
    font-weight: bold;
    font-size: 120%;
    height: 2.1rem;
    width: 2.1rem;
    box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.45);
    position: absolute;
    right: 0;
    top: 0;
    background-image: linear-gradient(to bottom right, #ffe9e9, #ff9393);
    border-radius: 50vw;
    text-align: center;
    color: #727272;
    display: flex;
    justify-content: center;
    align-items: center;
}

.rd-main {
    display: flex;
}

.rd-roundlbl {
    padding-right: 1rem;
    height: 4rem;
    border-radius: 100vw;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
}
.rd-roundlbl-cont {
    height: 4rem;
    border-radius: 100vw;
    background-image: linear-gradient(to right, #dad9d9, #ffffff);
    position: relative;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
}
.rd-roundlbl-cont .rd-roundlbl-fig,
.rd-roundlbl-cont .rd-roundlbl-lin,
.rd-roundlbl-cont .rd-roundlbl-lbl {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}
.rd-roundlbl-cont .rd-roundlbl-fig {
    left: 1rem;
    width: 2rem;
    text-align: center;
    font-weight: 600;
    font-size: 170%;
}
.rd-roundlbl-cont .rd-roundlbl-lin {
    left: 4rem;
    width: 1px;
    height: 60%;
}
.rd-roundlbl-cont .rd-roundlbl-lbl {
    left: 5rem;
}

