.mainClassRow{
    display: flex;
    flex-direction: column;
    height: calc(100vh - 18.8vh - 35px);
}
.mat-action-row {
    border: 0px;
    justify-content: flex-start;
    position: relative;
    padding: 0px;
    height: calc(50% - 4vh);
    margin-top: 4vh;
    align-items: inherit;
    
}

.mat-action-row mat-card {
    width: 100%;
    height: 100%;
    padding: 0px;
}
.ddFlex{
    height: calc(100% - 40px);
    width: 100%;
    display: inline-flex;
    align-items: center;
}
.matInnerClassFirst {
    margin: 0 2vh 0 0;
    width: calc(50% - 2vh);
}
.matInnerClassSecond {
    margin: 0 0 0 2vh;
    width: calc(50% - 2vh);
}
.padd2vh {
    padding: 19px;
    
}
.RaTitle{
    display: flex;
    align-items: center;
}
.RaTitleColor {
    color: rgb(224, 82, 35);
}
.fs-3vh {
    font-size: 3vh;
}
.fs-4vh {
    font-size: 4vh;
}
.padd2vh mat-card-content {
    margin: 0px;
}
