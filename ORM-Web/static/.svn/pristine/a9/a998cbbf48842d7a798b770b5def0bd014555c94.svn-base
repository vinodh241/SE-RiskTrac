.rd-tbl {
    width: 100%;
    box-shadow: 0px 4px 5px rgba(0, 0, 0, 0.45);
    border-top-left-radius: 1vmin;
    border-top-right-radius: 1vmin;
    border-bottom-left-radius: 1vmin;
    border-bottom-right-radius: 1vmin;
    overflow: hidden;
    border-collapse: collapse;
}
.rd-tbl-ordhead {
    width: 100%;
}
.rd-tbl td,
.rd-tbl th {
    padding: 0.29rem;
    padding-top: 3px;
    padding-bottom: 3px;
    vertical-align: middle;
    //text-align: left;
}
.rd-tbl-ordbody {
    background-image: linear-gradient(to right, #dad9d9, #fff);
}
.rd-tbl-ordbody td:nth-child(2),
.rd-tbl-ordbody td:nth-child(3) {
    font-size: 200%;
}
.rd-tbl-ordbody td {
    border-bottom: 1px solid #c6c6c6;
}
.rd-tbl tr:hover td {
    background-color: #fffdeb;
}
.rd-tbl tr:nth-child(odd) {
    background-color: #f3f3f3;
}

.txt-critical {
    color: #c01313;
}
.txt-moderate {
    color: #dc7a1f;
}
.txt-low {
    color: #096826;
}

.mat-header-row {
    background-image: linear-gradient(to right, #a8d7f5, #ffbfbf);
}
.top-bar {
    height: 0.3vh;
    width: 7vw;
}
.font-num {
    font-size: 150%;
    font-weight: bold;
    vertical-align: sub;
}
.centered-text {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
}
.ra-open {
    display: inline-block;
    background-color: #a90000;
    color: #fff;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
}
.ra-close {
    display: inline-block;
    background-color: #0e8a22;
    color: #fff;
    border-radius: 10px;
}

.ra-open:hover,
.top-bar:hover,
.ra-close:hover{
    text-decoration: none;
    color: #000;
}
.ra-gen {
    border-right: 1px solid #cdcdcd;
    display: inline-block;
    color: #727272;
    text-align: center;
    
}
.rd-tbl-data {
    color: #727272;
}
.rd-tbl-data:hover{
    text-decoration: none;
    color: #000;
}

.no-data {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
   
   
  }
  
  .no-data span {
    text-align: center;    
    margin-top: 35px;
    display: inline-block;
    color: #727272;
  }
  