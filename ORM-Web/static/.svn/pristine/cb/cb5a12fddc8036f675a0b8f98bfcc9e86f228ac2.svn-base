span.summaryGreen {
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    text-align: center;
    height: 4vh;
    border-radius: 3px 0 0 3px;
    background: rgb(16, 179, 136);
    background: -moz-linear-gradient(90deg, rgba(16, 179, 136, 1) 45%, rgba(39, 240, 168, 1) 100%);
    background: -webkit-linear-gradient(90deg, rgba(16, 179, 136, 1) 45%, rgba(39, 240, 168, 1) 100%);
    background: linear-gradient(90deg, rgba(16, 179, 136, 1) 45%, rgba(39, 240, 168, 1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#10b388", endColorstr="#27f0a8", GradientType=1);
}

span.summaryRed {
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    height: 4vh;
    text-align: center;
    border-radius: 0 3px 3px 0;
    background: rgb(205, 35, 35);
    background: -moz-linear-gradient(90deg, rgba(205, 35, 35, 1) 45%, rgba(249, 134, 134, 1) 100%);
    background: -webkit-linear-gradient(90deg, rgba(205, 35, 35, 1) 45%, rgba(249, 134, 134, 1) 100%);
    background: linear-gradient(90deg, rgba(205, 35, 35, 1) 45%, rgba(249, 134, 134, 1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#cd2323", endColorstr="#f98686", GradientType=1);
}

span.summaryYellow {
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    height: 4vh;
    text-align: center;
    border-radius: 0 3px 3px 0;
    background: rgb(200, 124, 11);
    background: -moz-linear-gradient(90deg, rgba(200, 124, 11, 1) 45%, rgba(249, 196, 134, 1) 100%);
    background: -webkit-linear-gradient(90deg, rgba(200, 124, 11, 1) 45%, rgba(249, 196, 134, 1) 100%);
    background: linear-gradient(90deg, rgba(200, 124, 11, 1) 45%, rgba(249, 196, 134, 1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#c87c0b", endColorstr="#f9c486", GradientType=1);
}
