	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 10px solid red;
}

.triangle-down {
	width: 0;
	height: 0;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-top: 10px solid green;
}

