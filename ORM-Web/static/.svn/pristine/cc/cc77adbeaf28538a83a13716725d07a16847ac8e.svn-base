.mat-container {
    height: 90%;
    width: 50px;
    border: 1px solid rgb(128, 126, 126);
    border-radius: 45px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background-color: aliceblue;
}

.mat-card-bg {
    background-image: linear-gradient(to right, #faf9f9, #f19f9f);
}

.kri-score {
    margin-bottom: -0px;
    margin-top: 10px;
    font-weight: 500;
    color: rgb(128, 126, 126);
    text-shadow: 2px 2px 4px rgb(128, 126, 126);
}

.expand-panel {
    background-image: linear-gradient(to right, #f2b4b4, #faf9f9, #a0f977);
}

.outer-container {
    padding-top: 5px;
    height: 100px;
    width: 50px;
    border: 1px solid rgba(0, 0, 0, 0.45);
    border-radius: 45px;
    display: flex;
    flex-direction: column;
    justify-content: end;
    // height: 100%;
    position: relative;
    display: inline-block;
    box-shadow: px 4px 5px rgba(0, 0, 0, 0.45);

    // box-shadow: 5px 5px 12px #888888
}

