body {
    font-family: "Open Sans Condensed";
    font-size: 18px;
    color: rgba(0, 0, 0, .8);
}
.mainClassRow{
    display: flex;
    flex-direction: column;
    height: calc(100vh - 18.8vh);
}
.mainClassRowInner{
    display:flex;  
    margin-top: 4vh; 
    height: calc(100vh - 18.8vh - 4vh - 37px);
}

.matInnerClassFirst {
    display:flex;  
    flex-direction: column; 
    height:100%;
    margin: 0 1.33vh 0 0; 
    width: calc(33.33% - 2.66vh);
}
.matInnerClassSecond {
    height:100%;
    margin: 0 2.66vh 0 2.66vh;
    width: calc(33.33% - 2.66vh);
}
.matInnerClassThird {
    display:flex; 
    flex-direction: column; 
    height:100%;
    margin: 0 0 0 1.33vh;
    width: calc(33.33% - 2.66vh);
}
.rd-redbox {
    background-image: linear-gradient(#E5C7D7, #F6F6F6);
    height: 120px;
    width: 120px;
    border-radius: 20px;
    text-align: center;
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
    position: relative;
}

.rd-redbox-inner {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.no-data{
    height: 150px;
    display: flex;
    align-items: center;
    justify-content: center;

}
.rd-redbox .rd-redbox-inner div {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    vertical-align: middle;
}

.rd-redbox .rd-redbox-inner div:nth-child(1) {
    font-weight: bold;
    color: #C55D04;
    font-size: 200%;
    line-height: 90%;
}

.rd-redbox .rd-redbox-inner div:nth-child(2) {
    color: #2d2d2d;
    font-size: 140%;
    line-height: 90%;
}

.rd-greybox {
    background-color: #F1F1F1;
    height: 120px;
    width: 120px;
    border-radius: 20px;
    text-align: center;
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
    position: relative;
}

.rd-greybox-inner {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #CB4900;
    font-weight: bold;
    font-size: 300%;
    text-shadow: 0px 5px 10px rgba(0, 0, 0, 0.45);
}

.rd-greybox .stroke-text {
    font-size: 70px;
    text-align: center;
    text-anchor: middle;
    stroke: #fff;
    stroke-width: 2px;
    fill: #C55D04;
}

.rd-progress {
    font-size: 18px;
}

.rd-progress .rd-progress-nos .rd-progress-nos-lft {
    color: #2D2D2D;
}

.rd-progress .rd-progress-nos .rd-progress-nos-rgt {
    color: #C55D04;
    font-size: 30px;
    font-weight: bold;
    float: right;
}

.rd-progress .rd-progress-nos .rd-progress-nos-big {
    font-size: 30px;
}

.rd-progress .rd-progress-base {
    box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.45);
    background-color: rgba(255, 255, 255, 0.30);
    height: 35px;
    border-radius: 100vw;
}

.rd-progress .rd-progress-bar {
    box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.45);
    background-image: linear-gradient(to bottom right, #FFAA2E, #CB4900);
    ;
    height: 35px;
    border-radius: 100vw;
}

.rd-greycircle {
    background-color: #F1F1F1;
    height: 160px;
    width: 160px;
    border-radius: 100%;
    text-align: center;
    box-shadow: 5px 5px 15px rgba(0, 0, 0, 0.3);
}

// .rd-nmploss{}
.rd-nmploss aside {
    width: 10px;
    height: 100px;
    border-radius: 50vw;
    background-image: linear-gradient(#FFAA2E, #CB4900);
}

.rd-nmploss .rd-nmploss-fs18 {
    font-size: 18px;
}

.rd-nmploss .rd-nmploss-fs30 {
    font-size: 30px;
}

.rd-roundlbl {
    padding-right: 1rem;
    height: 4rem;
    border-radius: 100vw;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
}

.rd-roundlbl-cont {
    height: 4rem;
    width: 100%;
    border-radius: 100vw;
    background-image: linear-gradient(to right, #DAD9D9, #FFFFFF);
    position: relative;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
}

.rd-roundlbl-cont .rd-roundlbl-fig,
.rd-roundlbl-cont .rd-roundlbl-lin,
.rd-roundlbl-cont .rd-roundlbl-lbl {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.rd-roundlbl-cont .rd-roundlbl-fig {
    left: 1.5rem;
    width: 2rem;
    text-align: center;
    font-weight: 600;
    font-size: 170%;
}

.rd-roundlbl-cont .rd-roundlbl-lin {
    left: 4.5rem;
    width: 1.4px;
    height: 60%;
}

.rd-roundlbl-cont .rd-roundlbl-lbl {
    left: 5.5rem;
}

.rd-cmgroup {
    height: 3rem;
    border-radius: 100vw;
    position: relative;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.45);
    padding: .2rem;
    background-color: #fff;
    display: table;
    width: 100%;
}

.rd-cmgroup-gn {
    height: 2.6rem;
    background-image: linear-gradient(to right, #DAD9D9, #FFFFFF);
    border-radius: 50vw;
    vertical-align: middle;
    display: table-cell;
    font-weight: bold;
    padding-left: .8rem;
}

.rd-cmgroup-fig {
    color: 727272;
    font-weight: bold;
    font-size: 180%;
    height: 3rem;
    width: 3rem;
    box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.45);
    position: absolute;
    right: 0;
    top: 0;
    background-image: linear-gradient(to bottom right, #FFE9E9, #FF9393);
    border-radius: 50vw;
    text-align: center;
    color: #727272;
}

.rd-kris1 {
    // background-image: linear-gradient(to bottom right, #F84B4B, #FFE0E0);
    box-shadow: 0px 4px 5px rgba(0, 0, 0, 0.45);
    border-radius: 50vw;
    height: 135px;
    width: 60px;
    text-align: center;
    position: relative;
    display: inline-block;
}

