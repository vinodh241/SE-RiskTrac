/* below colour code is for the nav filter */
.colorStatus.incidentReported {background: linear-gradient(100deg, #FFA73F 9.29%, #FF6565 100%);}
.colorStatus.scheduled {background: linear-gradient(100deg, #525252 9.29%, #8E8E8E 100%);}
.colorStatus.inprogress {background: linear-gradient(100deg, #22D8AC 9.29%, #318DFA 100%);}
.colorStatus.completed {background: linear-gradient(100deg, #49E086 9.29%, #11BC56 100%);}
.colorStatus.published {background: linear-gradient(100deg, #61BACD 9.29%, #346594 100%);}
.colorStatus.delayed {background: linear-gradient(100deg, #FF2D2D 9.29%, #FA31CE 100%);}


/* Incident Reports - #FF6565  - last color code from gradient
Scheduled - #8E8E8E - new one and gradient provided with mail
In Progress - #318DFA - last color code from gradient
Completed - #11BC56 - last color code from gradient
Published - #346594 - Little adjusted asp compare to last color
Delayed - #FF2C2D - First color code from gradient(page 35) */
/* background: linear-gradient(100deg, #FF2D2D 9.29%, #FA31CE 100%);
} */



.tabsLevel2 {right:15%; bottom: 0;}

.dropdown-menu.dropdownFilter {background-color: #67c7b5; width: 100%; font-size: 1.5rem;}
.navtabHeading {color: #fff;} .vmiddleLeft.navtabHeading {left: 1%;}
.nav-item.mutitabBg div {color: #fff; padding-top: 1vh; padding-bottom: 1vh; margin-right: 0.2vw; line-height: 1;}
.nav-item.mutitabBg button, .nav-item.mutitabBg button:focus, .nav-item.mutitabBg button:focus-visible {background-color: #00000010; text-align: center; color: #000; outline: none; border: none; padding-top: 1vh; padding-bottom: 1vh; margin-right: 0.2vw; cursor: pointer;}
.nav-item.mutitabBg button.active {background: linear-gradient(180deg, #F2F2F2 0%, #D7D7D7 100%); color: #000;}
.nav-item.mutitabBg.textonly {font-size: 1.4rem;} .nav-item.mutitabBg.textonly span {font-size: 2rem;}

.overallStatus {width: 58%; color: #fff;}
.textOnlytop {font-size: 1.4rem;} .textOnlytop span {font-size: 1.6rem;}
.dueText {color: #FBFF25;} .dueText span {color: #fff;}

/* END FILTER */

/* DROPDOWN MENU */
.dropdown-toggle.userdot::after {content: none;}

.menuContent {overflow: auto; display: grid; grid-template-columns: repeat(6, 1fr); grid-row-gap: 2vh; grid-column-gap: 1vw; grid-template-rows: max-content; grid-auto-rows: 1fr;}
.dropdown-menu-dark.gradientDropdown {background: linear-gradient(90deg, #2470A3 0.57%, #0A9C79 100%); box-shadow: 0px 1vh 2vh 0px rgba(0, 0, 0, 0.25); border-radius: 0.3vh 0.3vh 4vh 0.3vh; height: 26.5vh; padding: 2vh 1vw; right: 0;}

.dropdown-menu.dropdown-menu-dark.gradientDropdown.userLogin {left: auto; right: 0; height: max-content;}

.menuList {border-right: 1px solid #ffffff36; position: relative; font-size: 1em; padding: 0.5vh 1vw; margin-block: 0.2vh; width: 100%; height: 100%;} /*As per the number of content*/
.dropdown-menu.dropdown-menu-dark.gradientDropdown.userLogin .menuList {padding: 0;}
.menuList:last-child {border-right: none;}
.menuList::before {content: ""; display: block; width: 0.6em; height: 0.6em; right: -.36em; top: 0; position: absolute; background-color: #5093b2; border-radius: 50vh;}
.menuList:last-child::before {display: none;}
.menuListHead {color: #000; font-size: 2em;}
.menuList ul {padding-left: 0;}
.menuList ul li {list-style: none; font-size: 1.4em;}
/* .menuList ul li a::before {content:"\f178";  display: inline-block; font-family:'FontAwesome'; margin-right: 0.5em;} */
.dropdown-menu.dropdown-menu-dark.gradientDropdown.userLogin .menuList.listOne {min-width: 10vw;}
.menuList .dropdown-item {padding-block: 0.5em; border-radius: 0.3vh;}
.menuList .dropdown-item:focus, .dropdown-item:hover {background-color: #ffffff1c; }
/* END DROPDOWN MENU */
