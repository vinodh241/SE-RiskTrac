/* NAVIGATION */

.navbg {box-shadow: 0px 0px 20px 0px rgba(0, 0, 0, 0.25); width: 100%; font-size: 1.6rem;}
.navbar-brand.BCMTrac img {width: 8em;}
.bg-light.topmenuBg {background-color: transparent !important; padding-inline: 0; border-bottom: 1px solid #ffffff69;}
.rightBorder {border-right: 1px solid #ffffff69;}
.topnav img {vertical-align: middle;}

.topnav li:nth-child(1) img {width: 60%;}
.topnav li:nth-child(2) img {width: 22%;}
.topnav li:nth-child(3) img {width: 14%;}
.topnav li:nth-child(4) img {width: 14%;}
.topnav li:nth-child(5) img {width: 22%;}
.topnav li:nth-child(6) img {width: 22%;}
.topnav li:nth-child(7) img {width: 70%;}
.topnav li:nth-child(8) img {width: 75%;}
.userProfile {width: 18%;}

/* .topnav img:nth-child(1) {width: 1em;}  */
.navbar-light .navbar-nav.topnav .nav-item a.nav-link {color: #fff; margin-inline: 2%; text-align: center;}

.navbar-light .navbar-nav.topnav .nav-item.active a.nav-link, .navbar-light .navbar-nav.topnav .nav-item a.nav-link:hover {background-color: #ffffff30; border-radius: 50vh; text-decoration: none;}
.navbar-light .navbar-nav.topnav .nav-item:nth-child(1) a.nav-link, .navbar-light .navbar-nav.topnav .nav-item:nth-child(7) a.nav-link, .navbar-light .navbar-nav.topnav .nav-item:nth-child(8) a.nav-link {background-color: transparent; border-radius: unset;}

.collapse.navbar-collapse.show.forMnuToggle {background-color: #295659; width: 30%; max-width: 30%; text-align: left; position: absolute; right: 0; top: 100%;     z-index: 999;
  border-radius: 1vh;}
.collapse.navbar-collapse.show.forMnuToggle img {width: 1em;}
.collapse.navbar-collapse.show.forMnuToggle .navbar-nav.topnav .nav-item a.nav-link {margin-inline: 0; padding-left: 2%;}
.collapse.navbar-collapse.show.forMnuToggle .navbar-nav.topnav .nav-item a.nav-link {text-align: left !important;}
.collapse.navbar-collapse.show.forMnuToggle .navbar-nav.topnav .nav-item.active a.nav-link, .collapse.navbar-collapse.show.forMnuToggle .navbar-nav.topnav .nav-item a.nav-link:hover {border-radius: 0;}
.userdot {position: relative;}
/* .userdot::before {position: absolute; content: ''; background-color:#FF0000; border-radius:50%; width: 1vh; aspect-ratio: 1 / 1; pointer-events: none; right: 0;} */
/* END NAVIGATION */

/* FILTER */

.topFilter {font-size: 1.6rem;}
.nav-tabs.top-nav-tabs {border-bottom: none;}
.nav-link.topTabBg {background-color: #00000010; border: none; color: #000; cursor: pointer; font-size: 90%; padding: 0 1vw !important; margin-inline: 0.05vw;}
.nav-link:nth-child(1).topTabBg {margin-left: 0;}
.nav-tabs .nav-item.show.topTabBg .nav-link, .nav-tabs .nav-link.topTabBg.active {background-color: #ffffff30; border: none; color: #fff;}
.nav-tabs .nav-link.topTabBg:focus {outline: none;}

/* .tab-content.topTabContentBg {background-color: #ffffff30;} */

.tab-content.topTabContentBg {background: linear-gradient(180deg, #ffffff30 0%, rgba(231, 243, 252, 0.00) 100%);}



.iconRefresh {color: #fff;}
.iconPositionLeft {     left: 87vw !important;   position: absolute;}

.btnGen {background-color: #1d7c98; border-radius: 50vh; min-height: 2.9vh; height: 2.5vh; padding: 0.2vh 0.8vw; vertical-align: middle; color: #fff; line-height: 1.5; cursor: pointer; text-wrap: nowrap;}
/* .btnGen.ev {padding: 0.5vh 0.8vw;} */
.btnGen.cr {padding: 0.5vh 0.8vw;}
.btnGen img {vertical-align: text-bottom; width: 1.2em;margin-right: 1vh;margin-left: 1vh;}
.btnGenDown {background-color: #245A8C; border-radius: 50vh; min-height: 3.5vh; height: 3.5vh; padding: 0.2vh 0.8vw; vertical-align: middle; color: #fff; line-height: 1.5; cursor: pointer; text-wrap: nowrap;}
.btnGenUp {background-color: #3CBC60; border-radius: 50vh; min-height: 3.5vh; height: 3.5vh; padding: 0.2vh 0.8vw; vertical-align: middle; color: #fff; line-height: 1.5; cursor: pointer; text-wrap: nowrap;}
.btnGen {padding: 0.5vh 0.8vw;}
.btnGen.ev {padding: 0.5vh 0.8vw;}

