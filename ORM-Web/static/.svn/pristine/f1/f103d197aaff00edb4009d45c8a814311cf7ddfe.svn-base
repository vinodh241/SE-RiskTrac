<mat-toolbar style="width: auto; margin:-24px -24px 0px -24px; height: 8.5vh; font-size: 3.3vh;">
  <!-- <h2 mat-dialog-title>{{unitCount}} {{data.IsInprogress?"Units In progress":"Plans"}} in {{data.GroupName}} </h2> -->

  <h2 mat-dialog-title>{{unitCount}} {{data.IsInprogress?"Units" : "Plans"}} {{dialogText}} in {{data.GroupName}} </h2>

  <span class="spacer"></span>
  <button mat-icon-button title="Close" cdkFocusInitial mat-dialog-close>
    <mat-icon aria-hidden="false" aria-label="Close" fontIcon="close"></mat-icon>
  </button>
</mat-toolbar>
<div class="container">
    <div fxLayout="row wrap" fxLayoutGap="10px grid" fxFlex="98%" fxFlexOffset.lg="1%" fxLayoutAlign="start stretch">
                <table mat-table [dataSource]="dataSource">
                 <ng-container matColumnDef="RowNumber">
                    <th mat-header-cell *matHeaderCellDef>#</th>
                    <td mat-cell *matCellDef="let row;">{{ row.RowNumber }}</td>
                  </ng-container>
                  <ng-container matColumnDef="Description">
                    <th mat-header-cell *matHeaderCellDef>{{data.IsInprogress?"Unit":"Identified Action"}}</th>
                  <td mat-cell *matCellDef="let row;" >{{row.Description}}</td>
                  </ng-container>
                  <tr mat-row *matHeaderRowDef="['RowNumber','Description']"></tr>
                  <tr mat-row *matRowDef="let element; columns:['RowNumber','Description']"></tr>
                </table>
           
    </div>
</div>